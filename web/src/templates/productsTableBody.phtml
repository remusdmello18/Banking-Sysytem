<?php


foreach($products as $product) {
    $cost = $product->getCost() / 100;
    echo "
        <tr>
            <td>{$product->getStockKeepingUnit()}</td>
            <td>{$product->getName()}</td>
            <td>{$product->getCategory()->getName()}</td>
            <td>{$cost}</td>
            <td>{$product->getQuantity()}</td>
        </tr>
    ";
}
